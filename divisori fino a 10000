<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width">
  <title>Numeri primi fino a 10000</title>
</head>

<body>
  <button id="bottone" onclick="trovaNumeriPrimi()">Trova numeri primi</button>
  <div id="risultato"></div>

  <script>
    function trovaNumeriPrimi() {
      let risultatoDiv = document.getElementById('risultato');
      risultatoDiv.innerHTML = ""; 

      for (let numero = 2; numero <= 10000; numero++) {
        let divisori = 0;
        for (let i = 2; i * i <= numero; i++) {
          if (numero % i === 0) {
            divisori++;
            break; 
          }
        }
        if (divisori === 0) {
          risultatoDiv.innerHTML += numero + " Ã¨ un numero primo<br>";
        }
      }
    }
  </script>

</body>

</html>
